#import Random.sea

struct Person {

	char* name;
	int age;
    private u64 id;

	init(char* name, int age = 18) {
		this.name = name;
		this.age = age;
		this.id = Random.generateU64();
	}

	char* toString() {
		return "Person(name: ${this.name}, age: ${this.age}, id: ${this.id})";
	}

	bool equals(Person* other) {
		return this.id == other.id;
	}

	u64 getId() {
		return this.id;
	}

    static u64 generateU64() {
        return Random.generateU64();
    }

}